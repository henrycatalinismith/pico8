pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
function _init()
	r = 32
	cx = 63
	cy = 63
	f = 0
end

function _update()
	f += 1
end

function _draw()
	cls(0)
	circ(cx, cy, r, 7)

	for longitude = 1, 2 do
	for latitude = -r+1, r-1 do
		local length = sqrt(
   r^2 - latitude^2
  ) * 2
  local m = length / r
 	local px =
 		(cx
 		 +longitude
 		 +length/2
 		)
 	 - (f*m % length)
		local py = cy + latitude
		
		pset(px, py, 12)
	end
	end
	
end
