pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
function _init()
	r = 48
	cx = 63
	cy = 63
	a = 0
end

function _update()
	a += 0.05
	if a > 1 then
	 a = 0
 end
end

function _draw()
 cls(0)
 circfill(cx, cy, r, 12)
 
 for longitude = -r, r do
    
  width = sqrt(
   r^2 - longitude^2
  ) * 2
    
  local py = cy + longitude
  
  local x1 = cx - width/2
  local x2 = cx + width/2
  
  for px = x1, x2 do
  	if flr(px + a * width) % 2 == 0 then
  		pset(px, py, longitude%16)
  	end
  end
  
 end
 
 print(a, 1, 1, 7)
 
end
